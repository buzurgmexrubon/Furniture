{
  "IpRateLimit": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIPHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*:/api/*",
        "Period": "5s",
        "Limit": 10
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgreSQL": "Host=localhost;Port=5432;Database=FurnitureDB;Username=postgres;Password=<your_password>;",
    "RedisConnectionString": "127.0.0.1:6379"
  },
  "Domain": "https://buzruk.uz",
  "Jwt": {
    "Key": "This is my custom Secret key for authentication",
    "Issuer": "http://buzruk.uz"
  },
  "EskizUz": {
    "Email": "<account_name>@gmail.com",
    "Key": "<key>"
  },
  "RedisCacheOptions": {
    "Configuration": "localhost:6379",
    "ConfigurationOptions": {
      "ConnectTimeout": 5000,
      "ConnectRetry": 3
    }
  }

}
